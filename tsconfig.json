{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmitOnError": true,
    "outDir": "./",
    "rootDir": "./",
    "target": "es6",
    "skipLibCheck": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "./node_modules"
  ]
}
